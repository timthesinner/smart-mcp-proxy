{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run MCP Proxy in Command Mode",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/proxy",
      "args": [
        "-mode", "command",
        "-config", "${workspaceFolder}/configs/example-config.json"
      ],
      "env": {
        "MCP_PROXY_MODE": "command"
      },
      "console": "integratedTerminal"
    }
  ]
}